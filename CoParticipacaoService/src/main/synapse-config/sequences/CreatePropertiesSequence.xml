<?xml version="1.0" encoding="UTF-8"?>
<sequence name="CreatePropertiesSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <property description="fileNameProp" expression="$trp:FILE_NAME" name="fileName" scope="default" type="STRING"/>
    <property description="filePathProp" expression="$trp:FILE_PATH" name="filePath" scope="default" type="STRING"/>
    <log description="fileNameLog">
        <property expression="fn:concat('Processing file [ ', get-property('fileName'), ' ] ' )" name="message"/>
    </log>
    <log level="custom">
        <property expression="get-property('filePath')" name="File Path: "/>
    </log>
    <log description="logFile" level="full"/>
</sequence>
