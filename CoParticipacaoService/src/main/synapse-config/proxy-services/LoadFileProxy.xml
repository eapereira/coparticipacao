<?xml version="1.0" encoding="UTF-8"?>
<proxy name="LoadFileProxy" startOnLoad="true" transports="vfs" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <sequence key="CoParticipacaoProcessSequence"/>
        </inSequence>
        <outSequence>
            <log description="responseLog" level="full"/>
            <send/>
        </outSequence>
        <faultSequence>
            <sequence key="ErrorSequence"/>
        </faultSequence>
    </target>
    <parameter name="transport.vfs.Streaming">true</parameter>
    <parameter name="transport.PollInterval">5</parameter>
    <parameter name="transport.vfs.FileURI">file://C:/desenv/work/coparticipacao/sulamerica/muito-facil/input</parameter>
    <parameter name="transport.vfs.ContentType">text/plain</parameter>
    <parameter name="transport.vfs.ActionAfterProcess">MOVE</parameter>
    <parameter name="transport.vfs.MoveAfterFailure">file://C:/desenv/work/coparticipacao/sulamerica/muito-facil/failure</parameter>
    <parameter name="transport.vfs.ActionAfterFailure">MOVE</parameter>
    <parameter name="transport.vfs.FileNamePattern">^(.+)(\.txt|\.TXT)$</parameter>
    <parameter name="transport.vfs.MoveAfterProcess">file://C:/desenv/work/coparticipacao/sulamerica/muito-facil/output</parameter>
</proxy>
